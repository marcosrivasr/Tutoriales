<% include partials/_header %>

    <div class="container-fluid mt-5">
        <div class="row">
            <div class="col-md-5">
                <div class="card">
                    <div class="card-body">
                        <form action="/update/<%= data.id %> " method="post">
                            <div class="form-group ">
                                <input type="text " name="nombre" value="<%= data.nombre %>" class="form-control">
                            </div>
                            <div class="form-group ">
                                <input type="text " name="direccion" value="<%= data.direccion %>" class="form-control">
                            </div>
                            <div class="form-group ">
                                <input type="text " name="telefono" value="<%= data.telefono %>" class="form-control">
                            </div>
                            <button type="submit " class="btn btn-success">Actualizar</button>
                        </form>



                    </div>
                </div>
            </div>
        </div>

    </div>




    <% include partials/_footer %>